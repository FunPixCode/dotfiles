{
    "SAM template": {
        "description": "SAM template",
        "prefix": "samt",
        "body": [
            "AWSTemplateFormatVersion: \"2010-09-09\"",
            "Transform: AWS::Serverless-2016-10-31",
            "Description: >",
            "\t ${1}",
            "",
            "Parameters:",
            "\t${2}",
            "",
            "Resources:",
            "\t$0",
            "",
            "Outputs:"
        ]
    },
    "SAM parameter": {
        "description": "SAM parameter",
        "prefix": "samp",
        "body": [
            "${1:Name}:",
            "\tDescription: ${2}",
            "\tType: ${3|String,Number,List<Number>,CommaDelimitedList|}",
            "\tDefault: ${4}",
            "\tAllowedValues:",
            "\t\t- ${5}",
            "\t\t- ${6}",
            "${0}"
        ]
    },
    "SAM template output": {
        "description": "SAM template output",
        "prefix": "samo",
        "body": [
            "${1:Output}",
            "\tDescription: ${2:Description.}",
            "\tValue: ${3:Value}",
            "\tExport:",
            "\t\tName: ${4:Export name}",
            "${0}"
        ],
    },
    "SAM S3": {
        "description": "SAM S3",
        "prefix": "sams3",
        "body": [
            "${1:Resource}:",
            "\tType: AWS::S3::Bucket",
            "\tProperties:",
            "\t\tBucketName: ${2}",
            "${0}"
        ],
    }
}